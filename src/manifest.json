{
  "name": "Pauper Scholar",
  "description": "Differentiate between free-access and paid-access articles in search results on Google Scholar.",
  "icons": {
      "128": "icon128.png"
  },
  "version": "1.1",
  "permissions": [
    "storage", "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://scholar.google.com/*"],
      "js": ["injection.js"]
    }
  ],
  "manifest_version": 2,
  "options_page": "options.html"
}
